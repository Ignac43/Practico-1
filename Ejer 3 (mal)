#include <iostream>
using namespace std;
int main()
{

    int P1, P2, P3, F, PF, N;
    
    N=(P1+P2+P3+F+PF);
    
    cout<<"Ingrese la nota P1 (sobre 40): ";
    cin>>P1;
    cout<<"Ingrese la nota P2 (sobre 40): ";
    cin>>P2;
    cout<<"Ingrese la nota P3 (sobre 40): ";
    cin>>P3;
    
    cout<<"Ingrese la nota del final (F): ";
    cin>>F;
    cout<<"Ingrese la nota del proyecto final (Pf): ";
    cin>>PF;
    cout<<"La nota final es: "<<N;
    
    return 0;
}
